{
  "name": "myfirstfunnuxtproject",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Mariko",
  "private": true,
  "scripts": {
    "dev": "npm run prep && nuxt",
    "resizeimages": "cd ./scripts/ && node resizeimages.js && cd ../",
    "buildlisting": "cd ./scripts/ && node buildlisting.js && cd ../",
    "drafts": "cd ./scripts/ && node listdrafts.js && cd ../",
    "prep": "npm run resizeimages && npm run buildlisting && npm run drafts",
    "generate": "npm run prep && nuxt generate",
    "postgenerate": "echo \"www.travelingcircusofurbanism.com\" > ./docs/CNAME && git add ."
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/markdownit": "^1.2.1",
    "node-sass": "^4.9.0",
    "nuxt": "^1.0.0",
    "sass-loader": "^7.0.1",
    "sharp": "^0.20.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
