{
  "name": "travelingcircusofurbanism",
  "version": "1.0.0",
  "description": "Urban narratives and practices, collected through travel",
  "author": "Mariko Sugita & Jasper Stephenson",
  "private": true,
  "scripts": {
    "analyze": "nuxt build -a",
    "autopush": "git add . && git commit -m \"Auto-pushing files to live\" && git push",
    "dev": "nuxt",
    "g": "npm run generate",
    "generate": "nuxt-generate -b && echo \"www.travelingcircusofurbanism.com\" > ./docs/CNAME && mv ./docs/404/index.html ./docs/404.html && rm -rf ./docs/404/",
    "golive": "git pull && npm run generate && npm run autopush"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@nuxtjs/feed": "^1.1.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "axios": "^0.19.0",
    "figures": "^3.0.0",
    "mapbox-gl": "^1.2.1",
    "markdown-it": "^9.1.0",
    "node-sass": "^4.12.0",
    "nuxt": "^2.8.1",
    "nuxt-generate-cluster": "^2.6.0",
    "nuxt-i18n": "^6.0.1",
    "polylabel": "^1.0.2",
    "sass-loader": "^7.2.0",
    "sharp": "^0.23.0",
    "std-env": "^2.2.1",
    "supercluster": "^6.0.2",
    "vue-loader": "^15.7.1"
  },
  "devDependencies": {
    "@nuxtjs/sitemap": "^1.2.0",
    "vue-lazyload": "^1.3.2"
  }
}
