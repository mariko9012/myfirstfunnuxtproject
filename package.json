{
  "name": "travelingcircusofurbanism",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Mariko Sugita & Jasper Stephenson",
  "private": true,
  "scripts": {
    "analyze": "nuxt build -a",
    "autopush": "git add . && git commit -m \"Auto-pushing files to live\" && git push",
    "dev": "nuxt",
    "generate": "nuxt generate && echo \"www.travelingcircusofurbanism.com\" > ./docs/CNAME",
    "golive": "git add . && git commit -m \"Committing local\" && git pull && npm run generate && npm run autopush",
    "prep": "node ./scripts/postbot.js"
  },
  "dependencies": {
    "@nuxtjs/feed": "^0.1.1",
    "@nuxtjs/google-analytics": "^2.0.2",
    "axios": "^0.18.0",
    "mapbox-gl": "^0.45.0",
    "markdown-it": "^8.4.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "nuxt": "^1.4.2",
    "sass-loader": "^7.1.0",
    "sharp": "^0.20.8",
    "supercluster": "^4.1.1",
    "vue-loader": "^14.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.1",
    "vue-lazyload": "^1.2.6"
  }
}
