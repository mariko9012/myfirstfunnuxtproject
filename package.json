{
  "name": "myfirstfunnuxtproject",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Mariko",
  "private": true,
  "scripts": {
    "dev": "npm run prep && nuxt",
    "prep": "node ./scripts/postbot.js",
    "postbot": "nodemon ./scripts/postbot.js",
    "generate": "npm run prep && nuxt generate",
    "postgenerate": "echo \"www.travelingcircusofurbanism.com\" > ./docs/CNAME && git add ."
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/markdownit": "^1.2.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "nuxt": "^1.0.0",
    "sass-loader": "^7.0.1",
    "sharp": "^0.20.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "*/resized/*"
    ],
    "watch": [
      "static/posts"
    ],
    "ext": "jpg,jpeg,png,gif,webm,js,md"
  }
}
